{
    "academic_terms": [
        {"name": "id", "method": "None"},
        {"name": "canvas_id", "method": "None"},
        {"name": "name", "method": "None"},
        {"name": "date_start", "method": "None"},
        {"name": "date_end", "method": "None"}
    ],
    "course": [
        {"name": "id", "method": "ffx.encrypt"},
        {"name": "canvas_id", "method": "ffx.encrypt"},
        {"name": "term_id", "method": "None"},
        {"name": "name", "method": "faker.course"}
    ],
    "file_access": [
        {"name": "id", "method": "None"},
        {"name": "file_id", "method": "ffx.encrypt"},
        {"name": "user_id", "method": "ffx.encrypt"},
        {"name": "access_time", "method": "shuffle"}
    ],
    "file": [
        {"name": "id", "method": "ffx.encrypt"},
        {"name": "name", "method": "faker.file_name"},
        {"name": "course_id", "method": "ffx.encrypt"}
    ],
    "user": [
        {"name": "id", "method": "None"},
        {"name": "user_id", "method": "ffx.encrypt"},
        {"name": "name", "method": "faker.name"},
        {"name": "sis_id", "method": "ffx.encrypt"},
        {"name": "sis_name", "method": "faker.user_name"},
        {"name": "course_id", "method": "ffx.encrypt"},
        {"name": "current_grade", "method": "redist.course_id"},
        {"name": "final_grade", "method": "redist.course_id"}
    ],
    "assignment_groups": [
        {"name": "id", "method": "ffx.encrypt"},
        {"name": "name", "method": "faker.assignment"},
        {"name": "weight", "method": "redist.course_id"},
        {"name": "group_points", "method": "redist.course_id"}, 
        {"name": "course_id", "method": "ffx.encrypt"},
        {"name": "drop_lowest", "method": "None"},
        {"name": "drop_highest", "method": "None"}
    ],
    "join__assignment__submission" : {
        "where" : "assignment.id = submission.assignment_id",
        "tables" : {
            "assignment": [
                {"name": "id", "method": "ffx.encrypt"},
                {"name": "name", "method": "faker.assignment"},
                {"name": "due_date", "method": "faker.date_time_on_date"},
                {"name": "local_date", "method": "faker.date_time_on_date"},
                {"name": "points_possible", "method": "redist.assignment__course_id"},
                {"name": "course_id", "method": "ffx.encrypt"},
                {"name": "assignment_group_id", "method": "ffx.encrypt"}
            ],
            "submission": [
                {"name": "id", "method": "ffx.encrypt"},
                {"name": "assignment_id", "method": "ffx.encrypt"},
                {"name": "course_id", "method": "ffx.encrypt"},
                {"name": "user_id", "method": "ffx.encrypt"},
                {"name": "score", "method": "ffx.encrypt"},
                {"name": "graded_date", "method": "faker.date_time_on_date"},
                {"name": "avg_score", "method": "mean.submission__score____submission__assignment_id"}
            ]
        }
    },
    "assignment_weight_consideration": [
        {"name": "course_id", "method": "ffx.encrypt"},
        {"name": "consider_weight", "method": "None"}
    ],
    "course_view_option": [
        {"name": "course_id", "method": "ffx.encrypt"},
        {"name": "show_files_accessed", "method": "None"},
        {"name": "show_assignment_planning", "method": "None"},
        {"name": "show_grade_distribution", "method": "None"}
    ],
    "user_default_selection": [
        {"name": "id", "method": "ffx.encrypt"},
        {"name": "course_id", "method": "ffx.encrypt"},
        {"name": "user_id", "method": "ffx.encrypt"},
        {"name": "default_view_type", "method": "None"},
        {"name": "default_view_value", "method": "None"}
    ]
}